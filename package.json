{
  "name": "open-agent-skills",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "node scripts/generate-docs-index.mjs && next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply open-agent-skills-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply open-agent-skills-db --remote",
    "db:seed:local": "npx tsx scripts/generate-seed-sql.ts > seed.sql && npx wrangler d1 execute open-agent-skills-db --local --file=seed.sql",
    "db:studio": "drizzle-kit studio",
    "build:worker": "node scripts/generate-docs-index.mjs && opennextjs-cloudflare build",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare preview"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^16.1.1",
    "@next/third-parties": "^16.1.4",
    "drizzle-orm": "^0.45.1",
    "highlight.js": "^11.11.1",
    "next": "16.1.1",
    "next-intl": "^4.7.0",
    "next-mdx-remote": "^5.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "rehype-pretty-code": "^0.14.1",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.21.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.14.8",
    "@tailwindcss/postcss": "^4",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.58.0"
  }
}
